	<xsd:complexType name="Ephemeredes">
		<xsd:sequence>
		    <xsd:element name="type" type="catobj:ObjectTypeEnum" />
		    <xsd:element name="oem" type="ndm:oemType" />
		    <xsd:element name="opm" type="ndm:opmType" />
		    <xsd:element name="includesThirdPartyData"
		        type="xsd:boolean">
		        <xsd:annotation>
		            <xsd:documentation>
		                Flag indicating if information is from third
		                parties (orbit and/or maneuver
		            </xsd:documentation>
		        </xsd:annotation>
		    </xsd:element>
		    <xsd:element name="origin" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
				  Origin of the orbit information, 
				  in case of third party data it will be the data provider
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		    <xsd:element name="updatedBy" type="com:UpdatedByType">
		    </xsd:element>
		    <xsd:element name="orbitDeterminationEpoch"
		        type="ndm:epochType">
		    </xsd:element>
		    <xsd:element name="sigmaRMS" type="ndm:stateVectorType">
		        <xsd:annotation>
		            <xsd:documentation>
		                Time tagged 1-sigma RMS positional and velocity
		                uncertainty information in the OCRF
		            </xsd:documentation>
		        </xsd:annotation>
		    </xsd:element>
		    <xsd:element name="isAccuracyEnvelopeViolated"
		        type="xsd:boolean">
		        <xsd:annotation>
		            <xsd:documentation>
		                Flag indicating whether the accuracy envelope is
		                violated
		            </xsd:documentation>
		        </xsd:annotation>
		    </xsd:element>
		    <xsd:element name="AccuracyEnvelopeViolationEpoch"
		        type="ndm:epochType">
		    </xsd:element>
		    <xsd:element name="publicationDate" type="ndm:epochType">
		        <xsd:annotation>
		            <xsd:documentation>
		                Publication date of the subsequent data set
		            </xsd:documentation>
		        </xsd:annotation>
		    </xsd:element>
		    <xsd:element name="AccuracyEnvelope"
		        type="catperfa:AbstractAccuracyEnvelope">
		    </xsd:element>
		    <xsd:element name="maneuverFlag"
		        type="omc:ManeuverFlagType">
		    </xsd:element>
		    <xsd:element name="covarianceMatrixParameters"
		        type="eCCSDS:CovarianceMatrixParametersType">
		    </xsd:element>
		    <xsd:element name="id" type="xsd:long"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
